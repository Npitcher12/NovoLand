<button id="taskComplete">Click me to finish a task</button>
<button id="taskComplete2">Click me to finish a task</button>

<script>
    $("#taskComplete").on("click", function() {
        $.ajax({
            url: "/tasks/complete",
            type: "GET",
            data: {
              code: "T1"
            },
            success: function(data) {
                Swal.fire({
                    title: 'Task Completed!',
                    text: 'You have completed a task!',
                    icon: 'success',
                    confirmButtonText: 'Cool'
                })
            }
        });
    });
